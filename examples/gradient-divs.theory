Website
	theory main
		html
			div
				is gradientSet(0xff0000, 0x00ff00);
				div
				div
				div
				div
				div

		ff gradientSet(color1, color2)
			where count = 0, index = 0;
			any
					-> yield count = this.children.count;
				...
					|v style if (this.name == 'div') then { 'background-color' : (color1 + color2) * index / count + color1 } else {} endif
						yield index = index + 1;
