chris
  theory main
    html is myff(5);
      div

    ff myff(n)
      where chris = n * 5;
      any
        -> { chris: twoChris }
           where twoChris = 2 * chris
           yield threeChris = 3 * chris;
        ...
          -> 'this is next';
          div
            a
              span
        div
        john
          -> {};

    fn john(a, node) -> if (node.name == 'chris') then node.apply(dict, ret) else {} endif;
